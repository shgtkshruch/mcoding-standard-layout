- docs = {}
- docs[0] = {heading: 'HTML5', list: true, ul: ['追加された要素', '削除された要素', '意味が変わった要素', 'HTML5のコンテンツモデル']}
- docs[1] = {heading: 'CSS3', list: true, ul: ['追加されたプロパティ', '追加されたセレクタ', '追加された単位']}
- docs[2] = {heading: 'JavaScript', list: true, ul: ['JavaScriptの言語仕様', 'ECMAScriptとは？', 'jQueryとは？']}
- docs[3] = {heading: 'Sass, Less, Stylus', list: false}
- docs[4] = {heading: 'TypeScript', list: false}

h2.heading DOCUMENTS
ul.documents
  each doc, i in docs
    li
      h3.title= doc.heading
      if doc.list
        ul
          each list, j in doc.ul
            li
              a(href="#")= list
      else
        p 準備中
